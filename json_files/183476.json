                        "questions": [
                            {
                                "type": "extendedTextInteraction",
                                "label": "",
                                "prompt": {
                                    "body": "<p>Sommige mensen kiezen ervoor om geen kinderen te krijgen vanwege het milieu. <br />\n<br />\nWaarom zou dat werken? <br />\nWat vind jij zelf van deze keuze?</p>"
                                },
                                "feedback": null
                            }
                        ],
                        "responseDeclarations": [
                            {
                                "identifier": "6989966_1",
                                "cardinality": "single",
                                "baseType": "string",
                                "correctResponse": {
                                    "interpretation": null,
                                    "values": [
                                        {
                                            "fieldIdentifier": null,
                                            "value": "<p>Als er minder kinderen geboren worden, groeit de wereldbevolking misschien minder snel. Daardoor zal het verbruik van de aarde minder snel gaan en&nbsp;<em>dat</em>&nbsp;is beter voor het milieu.</p>\r\n"
                                        }
                                    ]
                                },
                                "mapping": {
