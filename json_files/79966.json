                        "questions": [
                            {
                                "type": "matchInteraction",
                                "label": "",
                                "prompt": {
                                    "body": "<p>Maak de zin compleet.</p>"
                                },
                                "shuffle": true,
                                "minAssociations": 5,
                                "maxAssociations": 5,
                                "sourceSimpleMatchSet": [
                                    {
                                        "identifier": "SOURCE_1",
                                        "content": {
                                            "body": "<p>makkelijk blijven hangen aan de vacht van dieren.</p>"
                                        },
                                        "minMatch": 1,
                                        "maxMatch": 1,
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "SOURCE_2",
                                        "content": {
                                            "body": "<p>zijn licht en verspreiden zich met behulp van de wind.</p>"
                                        },
                                        "minMatch": 1,
                                        "maxMatch": 1,
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "SOURCE_5",
                                        "content": {
                                            "body": "<p>door de zaaddoos open te laten springen, waardoor het zaad wegschiet.</p>"
                                        },
                                        "minMatch": 1,
                                        "maxMatch": 1,
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "SOURCE_4",
                                        "content": {
                                            "body": "<p>verspreidt het zaad zich, doordat het samen met de ontlasing het lichaam weer verlaat.</p>"
                                        },
                                        "minMatch": 1,
                                        "maxMatch": 1,
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "SOURCE_3",
                                        "content": {
                                            "body": "<p>gaat door de lucht als een propeller en is windverspreiding.</p>"
                                        },
                                        "minMatch": 1,
                                        "maxMatch": 1,
                                        "feedbackInline": null
                                    }
                                ],
                                "targetSimpleMatchSet": [
