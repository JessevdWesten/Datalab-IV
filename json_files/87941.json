                        "level": 2
                    },
                    "items": [
                        {
                            "type": "video",
                            "url": "https://www.youtube.com/watch?v=OMFk_9kkjjU",
                            "align": "left",
                            "caption": "Chippen van je hond",
                            "poster": "https://img.youtube.com/vi/OMFk_9kkjjU/maxresdefault.jpg"
