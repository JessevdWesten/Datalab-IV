                            "link": ""
                        },
                        {
                            "type": "image",
                            "src": "https://maken.wikiwijs.nl/userfiles/b/b831276467ee66705f4d56aa952e2fca266d4319.png",
                            "altText": "2.12 Leertekst 2",
                            "alignment": "center",
                            "scale": 0.3,
                            "link": ""
                        },
                        {
                            "type": "image",
                            "src": "https://maken.wikiwijs.nl/userfiles/5/560b544db1274bb98815e164ccd236855712f012.png",
                            "altText": "2.12 Leertekst 3",
                            "alignment": "center",
                            "scale": 0.3,
                            "link": ""
