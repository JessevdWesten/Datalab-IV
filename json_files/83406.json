                                        "cardinality": "single",
                                        "baseType": "string",
                                        "correctResponse": {
                                            "interpretation": null,
                                            "values": [
                                                {
                                                    "fieldIdentifier": null,
                                                    "value": "<p>Wat heeft de buurvrouw/man opgeschreven?</p>\r\n"
                                                }
                                            ]
                                        },
