                                                "questions": [
                                                    {
                                                        "type": "selectPointInteraction",
                                                        "label": "",
                                                        "prompt": {
                                                            "body": "<p>Op welke plaats van de hype cycle bevindt het <strong>vakoverstijgende dashboard</strong> zich?</p>"
                                                        },
                                                        "minNumberOfPins": 1,
                                                        "maxNumberOfPins": "1",
                                                        "image": {
                                                            "src": "https://maken.wikiwijs.nl/userfiles/7/7e4d26315ff22d02ba8cca500220aaaba5d3184c.png",
                                                            "width": "1406",
                                                            "height": "716"
                                                        },
                                                        "responseIdentifier": "7232979_1_1",
                                                        "feedback": [
                                                            {
                                                                "outcomeIdentifier": "FEEDBACK_CORRECT",
                                                                "identifier": "FBC_7232979_1",
                                                                "showHide": null,
                                                                "title": null,
                                                                "content": {
                                                                    "body": "Vakoverstijgende dashboards willen voorzien in het inzicht in het leerproces op scholen die veel digitale leermaterialen inzetten. Maar voor een dergelijk inzicht zijn veel afspraken nodig tussen veel verschillende partijen. Dit lukt in de praktijk nog onvoldoende waardoor de hoge verwachtingen nog niet worden waargemaakt."
                                                                }
                                                            },
                                                            {
                                                                "outcomeIdentifier": "FEEDBACK_INCORRECT",
                                                                "identifier": "FBI_7232979_1",
                                                                "showHide": null,
                                                                "title": null,
                                                                "content": {
                                                                    "body": "Vakoverstijgende dashboards willen voorzien in het inzicht in het leerproces op scholen waar veel gewerkt wordt met digitaal leermateriaal. Over dergelijke dashboards hebben veel potentieel en er wordt al een behoorlijke tijd over nagedacht. Toch maken zij op dit moment het potentieel nog niet waar omdat het maken van afspraken met alle partijen die nodig zijn om het dashboard te vullen in de praktijk zeer lastig te maken zijn."
                                                                }
                                                            }
                                                        ]
                                                    }
