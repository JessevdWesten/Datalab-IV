                                        "questions": [
                                            {
                                                "type": "matchInteraction",
                                                "label": "Sleep de juiste betekenis bij het spreekwoord of gezegde",
                                                "prompt": {
                                                    "body": "<p>Sleep de juiste betekenis op de plaats</p>"
                                                },
                                                "shuffle": true,
                                                "minAssociations": 20,
                                                "maxAssociations": 20,
                                                "sourceSimpleMatchSet": [
                                                    {
                                                        "identifier": "SOURCE_10",
                                                        "content": {
                                                            "body": "<p>Als je ergens kritiek op <strong>wil</strong> leveren, vind je altijd wel een reden</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_3",
                                                        "content": {
                                                            "body": "<p>Iemand niet meer zien</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_1",
                                                        "content": {
                                                            "body": "<p>De baas spelen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_8",
                                                        "content": {
                                                            "body": "<p>Iemands kleding bepaalt het aanzien</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_6",
                                                        "content": {
                                                            "body": "<p>Het is al bekend wie daar de winnaar is</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_11",
                                                        "content": {
                                                            "body": "<p>Zaken in hoog tempo afwerken, zorgt vaak voor fouten</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_19",
                                                        "content": {
                                                            "body": "<p>Een verwaarloosde opvoeding krijgen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_12",
                                                        "content": {
                                                            "body": "<p>Iets van plan zijn</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_14",
                                                        "content": {
                                                            "body": "<p>Iets uit je hoofd kennen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_17",
                                                        "content": {
                                                            "body": "<p>Iets verzwijgen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_5",
                                                        "content": {
                                                            "body": "<p>Oude, vergeten zaken weer terug laten komen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_16",
                                                        "content": {
                                                            "body": "<p>Jezelf druk maken om niet</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_20",
                                                        "content": {
                                                            "body": "<p>Heel vroeg uit bed bed gaan</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_2",
                                                        "content": {
                                                            "body": "<p>Je zin willen hebben</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_13",
                                                        "content": {
                                                            "body": "<p>Uitgebreid vertellen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_4",
                                                        "content": {
                                                            "body": "<p>Eerlijk zeggen wie het is. Niets verzwijgen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_7",
                                                        "content": {
                                                            "body": "<p>Kinderen lijken vaak op hun ouders</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_9",
                                                        "content": {
                                                            "body": "<p>Beter iets dan niets</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_15",
                                                        "content": {
                                                            "body": "<p>Iets verzinnen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "SOURCE_18",
                                                        "content": {
                                                            "body": "<p>Doen alsof er niets aan de hand is</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    }
                                                ],
                                                "targetSimpleMatchSet": [
                                                    {
                                                        "identifier": "TARGET_1",
                                                        "content": {
                                                            "body": "<p>Hij wil altijd de lakens uitdelen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_2",
                                                        "content": {
                                                            "body": "<p>Zijn haan moet altijd koning kraaien</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_3",
                                                        "content": {
                                                            "body": "<p>Ik heb hem uit het oog verloren</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_4",
                                                        "content": {
                                                            "body": "<p>Je kunt gerust man en paard noemen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_5",
                                                        "content": {
                                                            "body": "<p>Je moet geen oude koeien uit de sloot halen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_6",
                                                        "content": {
                                                            "body": "<p>Het was een spel van kat en muis</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_7",
                                                        "content": {
                                                            "body": "<p>De appel valt niet ver van de boom</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_8",
                                                        "content": {
                                                            "body": "<p>Kleren maken de man</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_9",
                                                        "content": {
                                                            "body": "<p>Beter een half ei dan een lege dop</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_10",
                                                        "content": {
                                                            "body": "<p>Wie een hond wil slaan, vindt altijd wel een stok</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_11",
                                                        "content": {
                                                            "body": "<p>Haatige spoed is zelden goed</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_12",
                                                        "content": {
                                                            "body": "<p>Wat voert hij nou weer in zijn schild?</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_13",
                                                        "content": {
                                                            "body": "<p>Hij vertelde alles in geuren en kleuren</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_14",
                                                        "content": {
                                                            "body": "<p>Hij schudt de uitkomsten zo uit zijn mouw</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_15",
                                                        "content": {
                                                            "body": "<p>Dat verhaal heeft hij uit zijn duim gezogen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_16",
                                                        "content": {
                                                            "body": "<p>Dat is echt een storm in een glas water</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_17",
                                                        "content": {
                                                            "body": "<p>De directie wilde dat in de doofpot stoppen</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_18",
                                                        "content": {
                                                            "body": "<p>Ze doet net of haar neus bloedt</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_19",
                                                        "content": {
                                                            "body": "<p>Die jongen groeit op voor galg en rad</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    },
                                                    {
                                                        "identifier": "TARGET_20",
                                                        "content": {
                                                            "body": "<p>Iedereen was voor dag en dauw opgestaan</p>"
                                                        },
                                                        "minMatch": 1,
                                                        "maxMatch": 1,
                                                        "feedbackInline": null
                                                    }
