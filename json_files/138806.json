            "heading": {
                "type": "heading",
                "content": "Beroepsopdracht 11 Handelt klachten af",
                "level": 1
            },
            "items": [
                {
                    "type": "file",
                    "title": "https://maken.wikiwijs.nl/userfiles/d/da5982b3c3a5de12e77abef617bb79f09c080a36.pdf",
                    "file": "https://maken.wikiwijs.nl/userfiles/d/da5982b3c3a5de12e77abef617bb79f09c080a36.pdf"
                }
