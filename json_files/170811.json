                                            {
                                                "type": "extendedTextInteraction",
                                                "label": "",
                                                "prompt": {
                                                    "body": "<p>Bereken de lengte van een rol koperdraad met een diameter van 1,5 mm en een massa van 10 kg. \u03c1<em><sub>koper</sub></em> = 8,96 g/mL.</p>"
                                                },
                                                "feedback": null
                                            }
                                        ],
                                        "responseDeclarations": [
