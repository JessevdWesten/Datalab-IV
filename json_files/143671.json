                                    }
                                ],
                                "feedback": [
                                    {
                                        "outcomeIdentifier": "FEEDBACK",
                                        "identifier": "ANSWER_CORRECT",
                                        "showHide": true,
                                        "title": null,
                                        "content": {
                                            "body": "<ol start=\"1\">\n<li>\u20ac 40,-</li>\n<li>9.000 stuks</li>\n<li>9,000 x \u20ac 15,- = \u20ac135.000,-</li>\n</ol>"
                                        }
