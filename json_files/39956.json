            "heading": {
                "type": "heading",
                "content": "De Schijf van vijf",
                "level": 1
            },
            "items": [
                {
                    "type": "image",
                    "src": "https://maken.wikiwijs.nl/userfiles/2/2b632b3e5ebd1eff3b07982a55f925b8.jpg",
