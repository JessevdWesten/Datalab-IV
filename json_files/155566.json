                    "assessmentItem": {
                        "identifier": "5783033",
                        "itemBody": null,
                        "questions": [
                            {
                                "type": "orderInteraction",
                                "label": "Handelingsvolgorde bij het uitvoeren van de hielprik",
                                "prompt": {
                                    "body": "<p>Zet de onderstaande onderdelen in de juiste volgorde.</p>\n<p> </p>"
                                },
                                "shuffle": true,
                                "minChoices": 0,
                                "maxChoices": 12,
                                "orientation": "vertical",
                                "choices": [
                                    {
                                        "identifier": "7277e44510f33087d07f4066d0ce308b",
                                        "content": {
                                            "body": "Pas handhygi\u00ebne toe."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1295359046aa972a407e41a1350316e7",
                                        "content": {
                                            "body": "Maak een schoon werkveld. Zet hierop de benodigdheden binnen handbereik."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "97094d4eb5162950ed0f771abed47e4d",
                                        "content": {
                                            "body": "Trek de handschoenen aan."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "d7a77ae98a95e9d53208463bcd77886a",
                                        "content": {
                                            "body": "Controleer of de testvlakken op de hielprikkaart ook aan de achterzijde volledig gevuld zijn. Overtollig bloed naast de testvlakken is toegestaan."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1e2e672eb78ed288439093e5698e2a57",
                                        "content": {
                                            "body": "Draag zorg voor de administratieve afhandeling.<br />\na. Doe de hielprikkaart in de antwoordenvelop. Verzend de antwoordenvelop op dezelfde dag.<br />\nb. Zorg dat de genummerde buitenenvelop minimaal drie maanden bewaard wordt."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "dfe582c18a9b571e85a6e834b1901d04",
                                        "content": {
                                            "body": "Laat het bloedmonster aan de lucht drogen."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "097bb6037e137f462f51b63b005f929b",
                                        "content": {
                                            "body": "Neem voldoende bloed af<br />\na. Houd de hielprikkaart tegen de druppel bloed en vul alle zes de testvlakken volledig met bloed.<br />\nb. Open het sneetje zo nodig door zachte massage. Voorkom onnodige massage. Prik zo nodig opnieuw"
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "c30aa1dc5f3463fb7218e14c34132d70",
                                        "content": {
                                            "body": "Ruim de materialen op."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "da5ae793aef8e389c54b345e29568d94",
                                        "content": {
                                            "body": "Plak de pleister over de prikplaats, zodat het sneetje zich goed kan sluiten."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1815d0886d3fddd35b602bc0054b868d",
                                        "content": {
                                            "body": "Plaats het lancet op de hiel en houd rekening met de richting van de massage, zodat de incisie (sneetje) zich opent."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "34a62d178ce02431f5a37d345a57d7a8",
                                        "content": {
                                            "body": "Verwijder het lancet van de hiel en gooi het in de afvalbak."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "ec46da4638d1a6bc9c7d601efcb88bd2",
                                        "content": {
                                            "body": "Druk met de wijsvinger de witte knop in: er wordt een incisie gemaakt. Het lancet wordt nu automatisch beschermd. De witte knop blijft ingedrukt."
                                        },
                                        "feedbackInline": null
                                    }
                                ],
                                "responseIdentifier": "5783033_1",
                                "feedback": [
                                    {
                                        "outcomeIdentifier": "FEEDBACK_CORRECT",
                                        "identifier": "FBC_5783033",
                                        "showHide": null,
                                        "title": null,
                                        "content": {
                                            "body": "Prima gedaan. Je hebt goed door hoe de handelingsvolgorde is."
                                        }
                                    },
                                    {
                                        "outcomeIdentifier": "FEEDBACK_INCORRECT",
                                        "identifier": "FBI_5783033",
                                        "showHide": null,
                                        "title": null,
                                        "content": {
                                            "body": "Bekijk het protocol van Vilans nog een keer."
                                        }
                                    }
                                ]
                            }
