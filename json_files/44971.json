            "heading": {
                "type": "heading",
                "content": "Periode 5: Aanleg en onderhoud van vijvers en waterpartijen",
                "level": 1
            },
            "items": [
                {
                    "type": "image",
                    "src": "https://maken.wikiwijs.nl/userfiles/5/535d5dfd662c378ed3ae3a7087cc6d50.jpg",
