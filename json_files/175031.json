                    "heading": {
                        "type": "heading",
                        "content": "Pitch, Jaw, Roll en Throtle",
                        "level": 2
                    },
                    "items": [
                        {
                            "type": "image",
                            "src": "https://maken.wikiwijs.nl/userfiles/2/2e78ff12f0afd9da788a393588374b85f882d94c.jpg",
