                    },
                    "heading": {
                        "type": "heading",
                        "content": "3 Rekenen-2",
                        "level": 2
                    },
                    "items": [
                        {
                            "type": "image",
                            "src": "https://maken.wikiwijs.nl/userfiles/7/77a1953d2fc6a3176a42ff5cc89025cd66c22dd0.png",
                            "altText": "",
                            "alignment": "right",
