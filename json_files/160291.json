            "heading": {
                "type": "heading",
                "content": "Ik heb een tekenbeet ",
                "level": 1
            },
            "items": [
                {
                    "type": "image",
                    "src": "https://maken.wikiwijs.nl/userfiles/5/5a79580b6507d20af871ea456c43585933aaac94.jpg",
