                    "heading": {
                        "type": "heading",
                        "content": "Het ontstaan van eieren ",
                        "level": 2
                    },
                    "items": [
                        {
                            "type": "image",
                            "src": "https://maken.wikiwijs.nl/userfiles/4/48565bb7271e03d7c74d956c04ef818d.JPG",
                            "altText": "",
                            "alignment": "right",
