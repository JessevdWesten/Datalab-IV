                        "questions": [
                            {
                                "type": "extendedTextInteraction",
                                "label": "Hypothese (jouw verwachting)",
                                "prompt": {
                                    "body": "<p>Denk jij dat leven op andere planeten in ons zonnestelsel mogelijk is?</p>"
                                },
                                "feedback": null
                            }
                        ],
                        "responseDeclarations": [
