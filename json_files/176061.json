                        "questions": [
                            {
                                "type": "extendedTextInteraction",
                                "label": "Hoe ging het?",
                                "prompt": {
                                    "body": "<p>Als het goed is weet jij nu veel meer over de planeten in ons zonnestelsel na het doen van zoveel onderzoek. Wist jij alle vragen bij de quiz? Of waren er toch een paar vragen bij waar jij nog niks van af wist?</p>"
                                },
                                "feedback": null
                            }
                        ],
                        "responseDeclarations": [
