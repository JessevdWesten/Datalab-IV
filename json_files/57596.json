                {
                    "reference": {
                        "value": "1076577"
                    },
                    "heading": {
                        "type": "heading",
                        "content": "Eerste oefening",
                        "level": 2
                    },
                    "items": [
                        {
                            "type": "video",
                            "url": "//player.vimeo.com/video/119109778",
                            "align": "left",
                            "caption": "Jans, Bert en Hans oefenen met Lync",
