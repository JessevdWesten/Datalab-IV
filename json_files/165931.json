                            "url": "https://www.youtube.com/embed/8YhD2UAaAzg?list=PLrB_TaQG_Ks8olzF6yZAhpetoLO2d-klo",
                            "align": "left",
                            "caption": "G3: Verzekeringen - Oplossingen (o.a. premiedifferentiatie, eigen risico en bonusmalus)",
                            "poster": "https://maken.wikiwijs.nl/res/images/playicon_wikiwijs.png"
