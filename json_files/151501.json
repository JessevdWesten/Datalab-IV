                        "questions": [
                            {
                                "type": "extendedTextInteraction",
                                "label": "multiculturaliteit",
                                "prompt": {
                                    "body": "<p>leg in eigen woorden uit wat het verschil is tussen integreren en segregeren en geef bij beide een voorbeeld.</p>"
                                },
                                "feedback": null
