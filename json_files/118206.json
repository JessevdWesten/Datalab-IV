            "items": [
                {
                    "type": "paragraph",
                    "content": "<p>Langdurige armoede</p>\n<p><iframe allowfullscreen=\"allowfullscreen\" frameborder=\"0\" height=\"315\" src=\"https://www.youtube.com/embed/igKkDVc8QKA\" width=\"560\" class=\"video-item\"/></p>\n"
