                        "itemBody": null,
                        "questions": [
                            {
                                "type": "extendedTextInteraction",
                                "label": "",
                                "prompt": {
                                    "body": "<p>Op welke manieren zorgde de VOC ervoor dat ze het monopolie in handen hielden?</p>"
                                },
                                "feedback": null
                            }
                        ],
