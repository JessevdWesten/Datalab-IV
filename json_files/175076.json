            },
            "heading": {
                "type": "heading",
                "content": "Dierentuin in de school",
                "level": 1
            },
            "items": [
                {
                    "type": "image",
                    "src": "https://maken.wikiwijs.nl/userfiles/3/36a32c424b6cdf6516e54fa643cc0b14863e5831.jpg",
                    "altText": "",
