                    "assessmentItem": {
                        "identifier": "1405438",
                        "itemBody": null,
                        "questions": [
                            {
                                "type": "orderInteraction",
                                "label": "The sun and the wind",
                                "prompt": {
                                    "body": "<p>Put the sentences in the right order</p>"
                                },
                                "shuffle": true,
                                "minChoices": 0,
                                "maxChoices": 11,
                                "orientation": "vertical",
                                "choices": [
                                    {
                                        "identifier": "dfe582c18a9b571e85a6e834b1901d04",
                                        "content": {
                                            "body": "The sun starts warming up everything and the man gets hot."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "d7a77ae98a95e9d53208463bcd77886a",
                                        "content": {
                                            "body": "The wind tries to blow the man's shawl away. But he can't."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "7277e44510f33087d07f4066d0ce308b",
                                        "content": {
                                            "body": "The wind and the sun were having an argument"
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "c30aa1dc5f3463fb7218e14c34132d70",
                                        "content": {
                                            "body": "The wind agrees that the sun wins the test."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "da5ae793aef8e389c54b345e29568d94",
                                        "content": {
                                            "body": "The man takes his shawl off."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "ec46da4638d1a6bc9c7d601efcb88bd2",
                                        "content": {
                                            "body": "The sun just smiles calmly."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1815d0886d3fddd35b602bc0054b868d",
                                        "content": {
                                            "body": "The wind can cover the sun with clouds, he says."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "34a62d178ce02431f5a37d345a57d7a8",
                                        "content": {
                                            "body": "The wind wants a test."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "97094d4eb5162950ed0f771abed47e4d",
                                        "content": {
                                            "body": "The wind has blown away many trees and has drowned many ships."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "097bb6037e137f462f51b63b005f929b",
                                        "content": {
                                            "body": "They are going to see who can take man's shawl off."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1295359046aa972a407e41a1350316e7",
                                        "content": {
                                            "body": "The argument was about who was the strongest of the two."
                                        },
                                        "feedbackInline": null
                                    }
                                ],
