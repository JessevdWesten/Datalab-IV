            "items": [
                {
                    "type": "question",
                    "reference": {
                        "value": "1396668"
                    },
                    "assessmentItem": {
                        "identifier": "1396668",
                        "itemBody": null,
                        "questions": [
                            {
                                "type": "orderInteraction",
                                "label": "A Song for Ben",
                                "prompt": {
                                    "body": "<p>Put these sentences in the right order</p>"
                                },
                                "shuffle": true,
                                "minChoices": 0,
                                "maxChoices": 18,
                                "orientation": "vertical",
                                "choices": [
                                    {
                                        "identifier": "2f9f97152351929cec960003f6f438aa",
                                        "content": {
                                            "body": "Katie locks the kidnappers in the trunk of the car."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "6e52173779ca703d66804474e8ff77e8",
                                        "content": {
                                            "body": "The kidnappers recognize Katie and take her too and put her in a room with Ben."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "0dcc40b95caa623cefab25f58665df81",
                                        "content": {
                                            "body": "Katie opens the door with a credit card."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "097bb6037e137f462f51b63b005f929b",
                                        "content": {
                                            "body": "Ben and Katie are not singing today because Katie is resting."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "97094d4eb5162950ed0f771abed47e4d",
                                        "content": {
                                            "body": "The stewardess says that she likes Katie's songs very much."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "b50d2add7d6c85c1299c949ac320e19a",
                                        "content": {
                                            "body": "On Tuesday Katie sees the man who kidnapped Ben and follows the car."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1e2e672eb78ed288439093e5698e2a57",
                                        "content": {
                                            "body": "The next day there is an article in the paper about Ben's kidnap. The kidnappers want a lot of money."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "7277e44510f33087d07f4066d0ce308b",
                                        "content": {
                                            "body": "A boy recognizes Katie Brown in a plane."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "34a62d178ce02431f5a37d345a57d7a8",
                                        "content": {
                                            "body": "Katie and Ben sing a song together."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "e131040357a3abbe915adc1a0aee1406",
                                        "content": {
                                            "body": "Katie sings her new song for Ben on the TV show."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "ec46da4638d1a6bc9c7d601efcb88bd2",
                                        "content": {
                                            "body": "Katie meets Ben."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "c30aa1dc5f3463fb7218e14c34132d70",
                                        "content": {
                                            "body": "Ben's parents ask Katie if she knows where Ben is"
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "da5ae793aef8e389c54b345e29568d94",
                                        "content": {
                                            "body": "Katie knows the registration number of the car and calls the police."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1295359046aa972a407e41a1350316e7",
                                        "content": {
                                            "body": "A stewardess is asking Katie what she wants to drink."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "1815d0886d3fddd35b602bc0054b868d",
                                        "content": {
                                            "body": "There are many journalists when she is leaving the plane"
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "dfe582c18a9b571e85a6e834b1901d04",
                                        "content": {
                                            "body": "Katie sees that the men are kidnapping Ben"
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "d7a77ae98a95e9d53208463bcd77886a",
                                        "content": {
                                            "body": "Two men looking at Ben and Katie."
                                        },
                                        "feedbackInline": null
                                    },
                                    {
                                        "identifier": "adae62b8e1c185243785e384063b6574",
                                        "content": {
                                            "body": "Ben and Katie drive to the studios with the kidnappers and there the police arrests them."
                                        },
                                        "feedbackInline": null
                                    }
                                ],
                                "responseIdentifier": "1396668_1",
                                "feedback": null
                            }
                        ],
