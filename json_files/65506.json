                "level": 1
            },
            "items": [],
            "sections": [
                {
                    "reference": {
                        "value": "1449484"
                    },
                    "heading": {
                        "type": "heading",
                        "content": "Bestaat schizofrenie?",
                        "level": 2
                    },
                    "items": [
                        {
                            "type": "link",
                            "link": "https://www.schizofreniebestaatniet.nl/",
                            "title": "Schizofrenie bestaat niet",
                            "description": "",
