                            {
                                "type": "choiceInteraction",
                                "label": "",
                                "prompt": {
                                    "body": "<p>Welke eigenschap van water maakt dat ijs op water drijft?</p>"
                                },
                                "shuffle": true,
                                "maxChoices": 4,
                                "minChoices": 1,
                                "orientation": "vertical",
                                "choices": [
                                    {
                                        "identifier": "0",
                                        "content": {
                                            "body": "IJs heeft een lagere dichtheid dan het water eronder."
                                        },
                                        "feedback": {
                                            "outcomeIdentifier": "FEEDBACK",
                                            "identifier": "0",
                                            "showHide": true,
                                            "title": null,
                                            "content": {
                                                "body": "Water bereikt de hoogste dichtheid bij 4 graden Celsius. IJS is kouder en heeft dus een lagere dichtheid waardoor het lichter is dan het water van een hogere temperatuur."
                                            }
                                        }
                                    },
                                    {
                                        "identifier": "1",
                                        "content": {
                                            "body": "Het ijs vormt waterstofbruggen met het water waardoor het niet zinkt."
                                        },
                                        "feedback": {
                                            "outcomeIdentifier": "FEEDBACK",
                                            "identifier": "1",
                                            "showHide": true,
                                            "title": null,
                                            "content": null
                                        }
                                    },
                                    {
                                        "identifier": "2",
                                        "content": {
                                            "body": "Door de viscositeit van water zinkt het ijs niet maar blijft het liggen op het water."
                                        },
                                        "feedback": {
                                            "outcomeIdentifier": "FEEDBACK",
                                            "identifier": "2",
                                            "showHide": true,
                                            "title": null,
                                            "content": null
                                        }
                                    },
                                    {
                                        "identifier": "3",
                                        "content": {
                                            "body": "Door de dissociatie worden de moleculen van het ijs afgestoten door het water."
                                        },
                                        "feedback": {
                                            "outcomeIdentifier": "FEEDBACK",
                                            "identifier": "3",
                                            "showHide": true,
                                            "title": null,
