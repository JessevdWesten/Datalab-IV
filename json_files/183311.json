                {
                    "reference": {
                        "value": "6981329"
                    },
                    "heading": {
                        "type": "heading",
                        "content": "Oefenen",
                        "level": 2
                    },
                    "items": [
                        {
                            "type": "paragraph",
                            "content": "<p>Nu je de formule gevonden hebt gaan we dit nu uitproberen aan de hand van een aantal opgaves.</p>\n<p>Bereken de oppervlakte van de rechthoeken in het volgende rechthoeken:</p>\n<p> </p>\n"
                        },
                        {
                            "type": "image",
                            "src": "https://maken.wikiwijs.nl/userfiles/4/4a382c2abf17589d905907f5d6a8055ff06d49fe.png",
                            "altText": "Rechthoeken",
                            "alignment": "center",
                            "scale": 0.7,
                            "link": ""
                        },
                        {
                            "type": "question",
                            "reference": {
                                "value": "6981357"
                            },
                            "assessmentItem": {
                                "identifier": "6981357",
                                "itemBody": null,
                                "questions": [
                                    {
                                        "type": "extendedTextInteraction",
                                        "label": "",
                                        "prompt": {
                                            "body": "<p>Geef de oppervlaktes van rechthoek 1, 2 en 3.</p>\n<p>Geef ook een berekening!</p>"
                                        },
                                        "feedback": null
                                    }
                                ],
                                "responseDeclarations": [
                                    {
                                        "identifier": "6981357_1",
                                        "cardinality": "single",
                                        "baseType": "string",
                                        "correctResponse": {
                                            "interpretation": null,
                                            "values": [
                                                {
                                                    "fieldIdentifier": null,
                                                    "value": "<p>Rechthoek 1 = 7 x 5 = 35</p>\r\n\r\n<p>Rechthoek 2 = 2 x 7 = 14</p>\r\n\r\n<p>Rechthoek 3 = 4 x 3 = 12</p>\r\n"
                                                }
                                            ]
                                        },
                                        "mapping": {
                                            "lowerBound": 0,
                                            "upperBound": 1,
                                            "default": 0,
                                            "mapEntries": []
                                        },
                                        "areaMapping": null
                                    }
                                ],
                                "outcomeDeclarations": [
                                    {
                                        "identifier": "SCORE",
                                        "cardinality": "single",
                                        "baseType": "float",
                                        "defaultValue": 0,
                                        "interpretation": null
                                    },
                                    {
                                        "identifier": "FEEDBACK",
                                        "cardinality": "single",
                                        "baseType": "identifier",
                                        "defaultValue": null,
                                        "interpretation": null
                                    }
                                ],
                                "feedback": [
                                    {
                                        "outcomeIdentifier": "FEEDBACK",
                                        "identifier": "ANSWER_CORRECT",
                                        "showHide": true,
                                        "title": null,
                                        "content": {
                                            "body": "<p>Rechthoek 1 = 7 x 5 = 35</p>\n<p>Rechthoek 2 = 2 x 7 = 14</p>\n<p>Rechthoek 3 = 4 x 3 = 12</p>"
                                        }
                                    }
                                ]
                            }
                        }
                    ],
                    "sections": []
                }
            ]
