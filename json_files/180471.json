                },
                {
                    "type": "video",
                    "url": "https://www.youtube.com/embed/SbusAtlh-WI",
                    "align": "left",
                    "caption": "Schaduw",
                    "poster": "https://maken.wikiwijs.nl/res/images/playicon_wikiwijs.png"
