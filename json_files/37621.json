            "heading": {
                "type": "heading",
                "content": "Ixperium",
                "level": 1
            },
            "items": [
                {
                    "type": "image",
                    "src": "https://maken.wikiwijs.nl/userfiles/4/46a0c7bbfc89a34af143f72297b34b3a.jpg",
                    "altText": "",
