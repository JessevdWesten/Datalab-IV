                            "file": "https://maken.wikiwijs.nl/userfiles/6/6c5ae7980a726b4ecb724dcc5b6c023b.pdf"
                        },
                        {
                            "type": "paragraph",
                            "content": "<table border=\"0\" cellspacing=\"0\">\n<tbody>\n<tr>\n<td> </td>\n<td>Digitaal leermateriaal checklist</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td><strong>Deze checklist kan je gebruiken om digitaal materiaal te toetsen</strong></td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td><strong>Deze checklist gaat in op het soort leermateriaal, Didactiek, Techniek en Inhoud<br/>\nen gaat door op de achterkant</strong></td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td><strong>Titel Website/arrangement:</strong></td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td><strong>Soort leermateriaal</strong></td>\n<td><strong>Ja</strong></td>\n<td><strong>Nee</strong></td>\n<td><strong>Opmerkingen</strong></td>\n</tr>\n<tr>\n<td>1</td>\n<td>Het materiaal is een stuk tekst/losse afbeelding</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>2</td>\n<td>Het materiaal is een lesonderdeel, fragment</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>3</td>\n<td>Het materiaal bevat 1 totale les (kop-staart)</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>4</td>\n<td>Het materiaal is onderdeel van een leerlijn</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td><strong>Didactiek</strong></td>\n<td><strong>Ja</strong></td>\n<td><strong>Nee</strong></td>\n<td><strong>Opmerkingen</strong></td>\n</tr>\n<tr>\n<td><strong>1</strong></td>\n<td>Kern of Leerdoel duidelijk</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>2</td>\n<td>Doelgroep duidelijk</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>3</td>\n<td>De leerling wordt geactiveerd/uitgedaagd</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>4</td>\n<td>De leerling wordt ge\u00efnformeerd/gemotiveerd</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>5</td>\n<td>Leerproces wordt gereguleerd ( uitleg/stappen)</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>6</td>\n<td>De leerroute is duidelijk / logisch</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>7</td>\n<td>De leerroute geeft ruimte voor personalisering</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>8</td>\n<td>Handleiding voor gebruiker/docent</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td><strong>Techniek</strong></td>\n<td><strong>Ja</strong></td>\n<td><strong>Nee</strong></td>\n<td><strong>Opmerkingen</strong></td>\n</tr>\n<tr>\n<td><strong>1</strong></td>\n<td>Mogellijkheden v.h. Internet worden goed benut</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>2</td>\n<td>Illustraties zijn functioneel</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>3</td>\n<td>Geluid is functioneel</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>4</td>\n<td>Hyperlinks werken, zijn actueel</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>5</td>\n<td>Navigatie is logisch</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>6</td>\n<td>Vormgeving is rustig</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>7</td>\n<td>Schermindeling is overzichtelijk</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>8</td>\n<td>Leerproces is technisch veilig</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>9</td>\n<td>Handleiding voor gebruiker/docent</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>10</td>\n<td>Materiaal is te downloaden</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>11</td>\n<td>Materiaal is aan te passen voor hergebruik</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>12</td>\n<td>Personalisering mogelijk</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td><strong>Inhoud/content</strong></td>\n<td><strong>Ja</strong></td>\n<td><strong>Nee</strong></td>\n<td><strong>Opmerkingen</strong></td>\n</tr>\n<tr>\n<td> </td>\n<td>De inhoud sluit aan op het gebruikte middel</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>2</td>\n<td>Inhoud sluit aan bij kern of leerdoelen</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>3</td>\n<td>Inhoud/taalgebruik sluit aan bij doelgroep</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>4</td>\n<td>De lesinhoud is juist</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>5</td>\n<td>De lesinhoud is actueel</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>6</td>\n<td>Gramatica en spelling correct?</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>7</td>\n<td>Teksten zijn helder /bondig</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>8</td>\n<td>Uitwisselbare blokken/hergebruik mogelijk?</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td>9</td>\n<td>Handleiding voor gebruiker/docent</td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td><strong>Wat is al goed? Of wat kan beter?</strong></td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n<tr>\n<td><strong>Antwoord:</strong></td>\n<td> </td>\n<td> </td>\n<td> </td>\n<td> </td>\n</tr>\n</tbody>\n</table>\n"
                        }
